	Issn = {1662-453X},
	Journal = {Frontiers in Neuroscience},
	Number = {00005},
	Title = {Rapid communication with a P300 matrix speller using electrocorticographic signals (ECoG)},
	Volume = {5},
	Year = {2011},
	Bdsk-Url-1 = {http://www.frontiersin.org/Journal/Abstract.aspx?s=763&name=neuroprosthetics&ART_DOI=10.3389/fnins.2011.00005},
